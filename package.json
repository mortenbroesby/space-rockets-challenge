  "scripts": {
    "start": "run-p start:*",
    "start:react": "react-scripts start",
    "start:test": "react-scripts test",
    "watch": "yarn start",
    "build": "react-scripts build",
    "test": "run-p test:*",
    "test:unit": "react-scripts test --watchAll=false",
    "test:lint": "yarn lint",
    "lint": "run-p lint:*",
    "lint:typescript": "tsc --project ./tsconfig.json --noEmit",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}' --fix",
